{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "https://student-info-management-backend.vercel.app/$1",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": {
              "Access-Control-Allow-Origin": "https://student-info-management-frontend.vercel.app"
            }
        }
    ]
}